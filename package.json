{
  "name": "MinecraftLauncher",
  "version": "1.0.8",
  "description": "Btw i'm a creeper",
  "main": "ts_output/render.js",
  "author": "Px",
  "license": "MIT",
  "private": false,
  "scripts": {
    "start-win": "npm run buildtsc && set APP_DEV=true && electron .",
    "start-linux": "APP_DEV=true && electron .",
    "build": "electron-builder",
    "buildtsc": "tsc && npm run lint",
    "lint": "npx eslint --fix"
  },
  "build": {
    "appId": "px.mclauncher",
    "mac": {
      "category": "public.app-category.utilities",
      "icon": "app_icns/icon.icns"
    },
    "win": {
      "asar": true,
      "target": "nsis",
      "icon": "app_icns/icon.png"
    }
  },
  "dependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@xmcl/core": "^2.9.0",
    "@xmcl/installer": "^4.2.0",
    "@xmcl/user": "^2.1.7",
    "adm-zip": "^0.5.9",
    "checksum": "^1.0.0",
    "data-store": "^4.0.3",
    "discord-rpc": "^4.0.1",
    "eslint": "^8.10.0",
    "jquery": "^3.6.0",
    "msmc": "^3.0.6",
    "node-fetch": "2",
    "request": "^2.88.2",
    "request-progress": "^3.0.0",
    "typescript": "^4.6.2",
    "unzipper": "^0.10.11",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "electron": "^17.0.1",
    "electron-builder": "^22.14.13"
  }
}
